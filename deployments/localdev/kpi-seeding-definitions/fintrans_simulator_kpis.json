{
  "fintrans_simulator_kpis": [
    {
      "kpi_name": "Transaction Total Rate",
      "kpi_formula": "rate(transactions_total[1m])",
      "kpi_definition": "Transaction rate per second across all services",
      "signal_type": "metrics",
      "query_type": "MetricsQL",
      "datastore": "victoriametrics",
      "layer": "impact",
      "classifier": "performance",
      "sentiment": "neutral"
    },
    {
      "kpi_name": "Transaction Failure Rate",
      "kpi_formula": "rate(transactions_failed_total[1m])",
