{
  "upi_business_top_10_kpis": [
    {
      "kpi_name": "National Transaction Success Rate",
      "kpi_formula": "sum(rate(transaction_total{success=\"true\"}[1h])) / sum(rate(transaction_total[1h])) * 100",
      "kpi_definition": "The percentage of successful transactions across India. Low rates indicate nationwide payment disruptions affecting millions of users' daily lives and business operations.",
      "layer": "impact",
      "classifier": "national_trust",
      "sentiment": "positive",
      "signal_type": "metrics",
      "query_type": "PromQL",
      "datastore": "victoriametrics",
      "emotional_impact": "Public confidence in digital payments",
      "business_impact": "Brand reputation and user adoption"
    },
    {
      "kpi_name": "Partner Bank Success Health Score",
      "kpi_formula": "avg by (OrgId) ((rate(transaction_total{success=\"true\", OrgId=~\".*\"}[1h]) / rate(transaction_total{OrgId=~\".*\"}[1h])) * 100 > bool 99.5)",
      "kpi_definition": "Measures how many partner banks are maintaining the 99.5% success rate SLA. Low scores indicate widespread banking partner issues affecting customer trust across multiple banks.",
      "layer": "impact",
      "classifier": "partner_ecosystem_health",
      "sentiment": "positive",
      "signal_type": "metrics",
      "query_type": "PromQL",
      "datastore": "victoriametrics",
      "emotional_impact": "Inter-bank customer trust",
      "business_impact": "Partner relationship management"
    },
    {
      "kpi_name": "Business Decline Rate by Bank",
      "kpi_formula": "sum by (OrgName) (rate(transaction_total{success=\"false\", error_code=~\"BD.*\"}[1h])) / sum by (OrgName) (rate(transaction_total[1h])) * 100",
      "kpi_definition": "Percentage of transactions declined due to business reasons (insufficient funds, account issues) per bank. High rates indicate customer financial stress or banking service issues.",
      "layer": "impact",
      "classifier": "customer_financial_health",
      "sentiment": "negative",
      "signal_type": "metrics",
      "query_type": "PromQL",
      "datastore": "victoriametrics",
      "emotional_impact": "Customer frustration and financial anxiety",
      "business_impact": "Bank customer satisfaction scores"
    },
    {
      "kpi_name": "Technical Failure Impact Score",
      "kpi_formula": "sum by (OrgName) (rate(transaction_total{success=\"false\", error_code=~\"TD.*\"}[1h])) * avg(transaction_amount)",
      "kpi_definition": "Monetary impact of technical failures per bank, considering transaction volume and average amount. High scores indicate significant revenue loss and customer inconvenience.",
      "layer": "impact",
      "classifier": "revenue_at_risk",
      "sentiment": "negative",
      "signal_type": "metrics",
      "query_type": "PromQL",
      "datastore": "victoriametrics",
      "emotional_impact": "Customer anger and loss of trust",
      "business_impact": "Direct revenue loss and recovery costs"
    },
    {
      "kpi_name": "Peak Hour Transaction Resilience",
      "kpi_formula": "avg(rate(transaction_total{success=\"true\"}[30m]) and hour() >= 19 and hour() <= 21) / avg(rate(transaction_total[30m]) and hour() >= 19 and hour() <= 21) * 100",
      "kpi_definition": "Success rate during evening peak hours (7-9 PM) when most Indians transact. Low resilience indicates system cannot handle prime-time load, affecting family expenses and emergency payments.",
      "layer": "impact",
      "classifier": "peak_time_reliability",
      "sentiment": "positive",
      "signal_type": "metrics",
      "query_type": "PromQL",
      "datastore": "victoriametrics",
      "emotional_impact": "Evening family payment disruptions",
      "business_impact": "Prime-time revenue assurance"
    },
    {
      "kpi_name": "Bank Performance Ranking",
      "kpi_formula": "topk(5, sum by (OrgName) (rate(transaction_total{success=\"true\"}[1h])) / sum by (OrgName) (rate(transaction_total[1h])) * 100)",
      "kpi_definition": "Real-time ranking of top 5 performing banks by success rate. Helps identify best practices and sets performance benchmarks across the banking ecosystem.",
      "layer": "impact",
      "classifier": "competitive_benchmarking",
      "sentiment": "positive",
      "signal_type": "metrics",
      "query_type": "PromQL",
      "datastore": "victoriametrics",
      "emotional_impact": "Bank pride and competitive spirit",
      "business_impact": "Performance-driven improvements"
    },
    {
      "kpi_name": "Critical Failure Escalation Index",
      "kpi_formula": "sum(rate(transaction_total{success=\"false\", error_code=~\"TD0[1-5]\"}[15m])) > 100",
      "kpi_definition": "Triggers when critical technical failures (TD01-TD05) exceed 100 in 15 minutes. Indicates system-wide issues requiring immediate executive attention and public communication.",
      "layer": "impact",
      "classifier": "crisis_management",
      "sentiment": "negative",
      "signal_type": "metrics",
      "query_type": "PromQL",
      "datastore": "victoriametrics",
      "emotional_impact": "National payment system anxiety",
      "business_impact": "Regulatory scrutiny and media impact"
    },
    {
      "kpi_name": "Small Merchant Success Rate",
      "kpi_formula": "sum(rate(transaction_total{success=\"true\", transaction_type=\"merchant_payment\", amount < 500}[1h])) / sum(rate(transaction_total{transaction_type=\"merchant_payment\", amount < 500}[1h])) * 100",
      "kpi_definition": "Success rate for small payments (< â‚¹500) to merchants. Critical for street vendors, small shops - the backbone of Indian retail economy.",
      "layer": "impact",
