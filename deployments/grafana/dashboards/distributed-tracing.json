{
  "dashboard": {
    "id": null,
    "title": "Mirador Core v7.0.0 - Distributed Tracing",
    "tags": ["mirador-core", "tracing", "opentelemetry"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Active Traces",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(mirador_core_traces_active_total)",
            "legendFormat": "Active Traces"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Trace Completion Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(mirador_core_traces_completed_total[5m])) / sum(rate(mirador_core_traces_started_total[5m])) * 100",
            "legendFormat": "Completion Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Query Trace Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(mirador_core_query_trace_duration_seconds_bucket[5m])) by (le, query_type))",
            "legendFormat": "{{query_type}} - 95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(mirador_core_query_trace_duration_seconds_bucket[5m])) by (le, query_type))",
            "legendFormat": "{{query_type}} - 50th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 12,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Correlation Trace Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(mirador_core_correlation_trace_duration_seconds_bucket[5m])) by (le, correlation_type))",
            "legendFormat": "{{correlation_type}} - 95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(mirador_core_correlation_trace_duration_seconds_bucket[5m])) by (le, correlation_type))",
            "legendFormat": "{{correlation_type}} - 50th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 12,
          "w": 24,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 5,
        "title": "Span Count by Operation",
        "type": "table",
        "targets": [
          {
            "expr": "sum(rate(mirador_core_spans_created_total[5m])) by (operation)",
            "legendFormat": "{{operation}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "spans/sec"
          }
        },
        "gridPos": {
          "h": 12,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 6,
        "title": "Trace Errors",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(mirador_core_trace_errors_total[5m])) by (error_type)",
            "legendFormat": "{{error_type}} errors/sec"
          }
        ],
        "gridPos": {
          "h": 12,
          "w": 12,
          "x": 12,
          "y": 32
        }
      },
      {
        "id": 7,
        "title": "Service Dependencies",
        "type": "nodeGraph",
        "targets": [
          {
            "expr": "sum(rate(mirador_core_service_calls_total[5m])) by (source_service, target_service)",
            "legendFormat": "{{source_service}} -> {{target_service}}"
          }
        ],
        "gridPos": {
          "h": 12,
          "w": 24,
          "x": 0,
          "y": 44
        }
      },
      {
        "id": 8,
        "title": "Trace Sampling Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(mirador_core_traces_sampled_total[5m])) / sum(rate(mirador_core_traces_started_total[5m])) * 100",
            "legendFormat": "Sampling Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 56
        }
      },
      {
        "id": 9,
        "title": "Trace Export Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(mirador_core_trace_exports_total{status=\"success\"}[5m])) / sum(rate(mirador_core_trace_exports_total[5m])) * 100",
            "legendFormat": "Export Success Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 56
        }
      },
      {
        "id": 10,
        "title": "Trace Buffer Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "mirador_core_trace_buffer_size",
            "legendFormat": "Buffer Size"
          },
          {
            "expr": "mirador_core_trace_buffer_capacity",
            "legendFormat": "Buffer Capacity"
          }
        ],
        "gridPos": {
          "h": 12,
          "w": 12,
          "x": 0,
          "y": 64
        }
      },
      {
        "id": 11,
        "title": "Span Attributes Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(mirador_core_span_attributes_total[5m])) by (attribute_name)",
            "legendFormat": "{{attribute_name}}"
          }
        ],
        "gridPos": {
          "h": 12,
          "w": 12,
          "x": 12,
          "y": 64
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}